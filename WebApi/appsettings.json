{
  /*
  * dotnet publish -c Release -r win10-x64 -p:PublishSingleFile=true --self-contained true
  * 发布单文件应用，必须拷贝 单文件输出目录下的文件到%Temp%/.net/对应应用名称/
  * 且 某些配置读取的是当前目录的位置的文件也需要从%Temp%/.net/对应应用名称/ 拷贝出来
  * .csproj PropertyGroup-IncludeSymbolsInSingleFile 将符号文件绑定到单文件里（默认不绑定）
  * .csproj ItemGroup-Content-ExcludeFromSingleFile 排除文档绑定到 单文件里
  * appsettings.json 必须排除在单文件 绑定外，否则无法运行
  * Kestrel证书文件 应排除在单文件绑定外.csproj ItemGroup-Content-ExcludeFromSingleFile=true
  */
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "urls": "http://*:6000;https://*:6001",
  "ConnectionStrings": {
    //Database：必须存放在C:\Users\{user}目录下NetCoreTempWebApi；AttachDBFilename：db文件目录（自由存放）
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;AttachDBFilename=%DataDirectory%\\NetCoreTempWebApi.mdf;Trusted_Connection=True;ConnectRetryCount=1",
    //"DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=NetCoreTempWebApi;Trusted_Connection=True;MultipleActiveResultSets=true",
    "RedisConnection": "localhost:6379,password=Michael,allowAdmin=true,connectTimeout=3000,connectRetry=3"
  },
  "JWT": {
    "ServerSecret": "My_ServerSecret123",
    "Issuer": "My_Issuer",
    "Audience": "My_Audience"
  },
  "Kestrel": {
    "Endpoints": {
      "http": {
        "Url": "http://+:3000"
      },
      "https": { // HttpsDefaultCert
        "Url": "https://+:3001",
        "Certificate": {
          //使用 单文件发布，证书文件 应排除在单文件绑定外.csproj ItemGroup-Content-ExcludeFromSingleFile=true
          "Path": "Certificate\\MServer.pfx",
          "Password": "Michael"
        }
      }
