# You can configure your scheduler in either<quartz> configuration section
# or in quartz properties file
# Configuration section has precedence

#指定调度器的实例名称
quartz.scheduler.instanceName = MyQuartzScheduler
quartz.jobStore.type = Quartz.Simpl.RAMJobStore, Quartz

# configure thread pool info
quartz.threadPool.type = Quartz.Simpl.SimpleThreadPool, Quartz
#设置线程池的最大线程数量
quartz.threadPool.threadCount = 3
#设置作业中每个线程的优先级，可取 System.Threading.ThreadPriority 中的枚举
quartz.threadPool.threadPriority = Normal

# job initialization plugin handles our xml reading, without it defaults are used 2.X 方式
#quartz.plugin.xml.type = Quartz.Plugin.Xml.XMLSchedulingDataProcessorPlugin, Quartz
##指定 Quartz 的作业配置文件路径及名称，以上指定为 quartz_jobs.xml，默认情况下命名为该名称即可。
#quartz.plugin.xml.fileNames = ~/Quartz/quartz_jobs.xml
#
quartz.plugin.xml.type = Quartz.Plugin.Xml.XMLSchedulingDataProcessorPlugin, Quartz.Plugins
#指定 Quartz 的作业配置文件路径及名称，以上指定为 quartz_jobs.xml，默认情况下命名为该名称即可。
quartz.plugin.xml.fileNames = ~/Quartz/quartz_jobs.xml

